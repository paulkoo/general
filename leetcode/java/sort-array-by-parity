class Solution {
    public int[] sortArrayByParity(int[] A) {
        int temp;
        
        for (int i = 0, j = 0; i < A.length; i++) {
            if (A[i] % 2 == 0) {
                temp = A[i];
                A[i] = A[j];
                A[j++] = temp;
            }
                
        }
        return A;
    }
}
